{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Relation Footnote",
  "type": "object",
  "required": ["relation", "target"],
  "properties": {
    "relation": {
      "type": "string",
      "minLength": 1
    },
    "target": {
      "type": "string",
      "minLength": 1
    },
    "confidence": {
      "type": "number",
      "minimum": 0,
      "maximum": 1
    }
  },
  "additionalProperties": false
}
